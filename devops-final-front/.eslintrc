{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  // "parserOptions": {
  //   "ecmaFeatures": {
  //     "jsx": true
  //   },
  //   "ecmaVersion": 12,
  //   "sourceType": "module"
  // },
  // "plugins": [
  //   "react"
  // ],
  "rules": {
    "no-unused-vars": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "jsx-a11y/alt-text": "warn",
    "react/prop-types": "off"
  },

    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "plugins": ["react", "react-hooks"]
}
